<h1>Listing app_keys</h1>

<table>
  <tr>
    <th>App</th>
    <th>App url</th>
    <th>Entity name</th>
    <th>App api token</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_keys.each do |app_key| %>
  <tr>
    <td><%= app_key.app %></td>
    <td><%= app_key.app_url %></td>
    <td><%= app_key.entity_name %></td>
    <td><%= app_key.app_api_token %></td>
    <td><%= link_to 'Show', app_key %></td>
    <td><%= link_to 'Edit', edit_app_key_path(app_key) %></td>
    <td><%= link_to 'Destroy', app_key, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App key', new_app_key_path %>
